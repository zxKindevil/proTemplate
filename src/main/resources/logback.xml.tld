<?xml version="1.0" encoding="UTF-8"?>

<configuration>
    <appender name="console">
        <encoder>
            <pattern>[%d{yyyy-MM-dd HH:mm:ss} %5p %c:%L] %m%n</pattern>
        </encoder>
    </appender>

    <root level="debug">
        <appender-ref ref="console" />
    </root>
</configuration>